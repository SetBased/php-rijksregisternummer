{
  "name": "setbased/rijksregisternummer",
  "description": "Library for handling identification numbers of the National Register of Belgium (a.k.a. Rijksregisternummer)",
  "keywords": [
    "Rijksregisternummer",
    "bisnummer",
    "bis-nummer",
    "BIS"
  ],
  "license": "MIT",
  "require": {
    "php": ">=7.3",
    "ext-mbstring": "*",
    "setbased/exception": "^2.1"
  },
  "require-dev": {
    "phing/phing": "^2.0",
    "phpunit/phpunit": "^9.0"
  },
  "autoload": {
    "psr-4": {
      "SetBased\\Rijksregisternummer\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SetBased\\Rijksregisternummer\\Test\\": "test/"
    }
  },
  "config": {
    "bin-dir": "bin/",
    "sort-packages": true
  }
}
