{
  "name": "setbased/rijksregisternummer",
  "description": "Library for handling identification numbers of the National Register of Belgium (a.k.a. Rijksregisternummer)",
  "keywords": [
    "Rijksregisternummer",
    "bisnummer",
    "bis-nummer",
    "BIS"
  ],
  "license": "MIT",
  "require": {
    "php": ">=7.4",
    "setbased/exception": "^2.2.0"
  },
  "require-dev": {
    "phing/phing": "^2.17.4",
    "phpstan/phpstan": "^1.9.4",
    "phpunit/phpunit": "^9.5.27"
  },
  "autoload": {
    "psr-4": {
      "SetBased\\Rijksregisternummer\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SetBased\\Rijksregisternummer\\Test\\": "test/"
    }
  },
  "config": {
    "bin-dir": "bin/",
    "sort-packages": true
  }
}
